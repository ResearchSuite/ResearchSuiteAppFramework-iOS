{
  "identifier": "main",
  "type": "tabBar",
  "onLoad": [],
  "tabs": [
    {
      "type": "list",
      "title": "Surveys",
      "shortTitle": "Surveys",
      "items": [
        {
          "identifier": "baseline",
          "title": "Baseline Survey",
          "predicate": {
            "format": "$baselineDate == nil",
            "substitutions": {
              "baselineDate": {
                "type": "valueInState",
                "identifier": "baselineCompletionDate"
              }
            }
          },
          "onTap": [
            {
              "type": "launchActivity",
              "activityID": "baseline"
            }
          ]
        },
        {
          "identifier": "daily",
          "title": "Daily Survey",
          "predicate": {
            "format": "$baselineDate != nil",
            "substitutions": {
              "baselineDate": {
                "type": "valueInState",
                "identifier": "baselineCompletionDate"
              }
            }
          },
          "onTap": [
            {
              "type": "launchActivity",
              "activityID": "dailyStressAssessment"
            }
          ]
        }
      ]
    },
    {
      "type": "list",
      "title": "Settings",
      "shortTitle": "Settings",
      "items": [
        {
          "identifier": "logOut",
          "title": "Log Out",
          "onTap": [
            {
              "type": "launchActivity",
              "activityID": "baseline"
            }
          ]
        }
      ]
    }
  ]
}
